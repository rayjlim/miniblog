{
  "name": "miniblog",
  "version": "2.2.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.4",
    "date-fns": "^2.28.0",
    "marked": "^4.0.11",
    "node-sass": "^7.0.1",
    "react": "^16.13.1",
    "react-date-picker": "^8.4.0",
    "react-dom": "^16.13.1",
    "react-md": "^2.2.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1"
  },
  "devDependencies": {
    "react-styleguidist": "^11.1.8"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "REACT_APP_API_ENDPOINT=http://localhost/projects/miniblog/backend REACT_APP_UPLOAD_ROOT=http://localhost/projects/miniblog/uploads PORT=3005 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "npx jsdoc -c conf.json",
    "styleguidist": "npx styleguidist server",
    "stop": "ps aux | grep styleguidist | grep -v grep | awk '{print $2}' | xargs kill -9",
    "kill": "ps ax | grep react-scripts | grep -v grep | awk '{print $1}' | xargs kill -9",
    "lint": "prettier src/ --write --single-quote --arrow-parens avoid && eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
