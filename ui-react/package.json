{
  "name": "miniblog",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost/projects/miniblog/",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.0.2",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/react": "^4.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.0.0",
    "react-md": "^1.16.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "better-docs": "^1.4.7",
    "jsdocs": "^1.0.0",
    "react-styleguidist": "^11.0.5"
  },
  "scripts": {
    "start": "PORT=3001  react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "npx jsdoc -c conf.json",
    "styleguidist": "npx styleguidist server",
    "stop": "ps aux | grep styleguidist | grep -v grep | awk '{print $2}' | xargs kill -9",
    "kill": "ps aux | grep react-scripts | grep -v grep | awk '{print $2}' | xargs kill -9",
    "lint": "prettier src/ --write --single-quote --arrow-parens avoid && eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
